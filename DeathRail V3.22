local function PcgetZombieNearMouse()
	local nearestZombie = nil
	local minScreenDistance = math.huge
	local closestZombie = nil
	local screenRadius = 2
	local playerPosition = HMNRT.Position
	local function GetNearFungtion(EnamyModel)
		if EnamyModel:IsA("Model") and EnamyModel:FindFirstChild("HumanoidRootPart") and EnamyModel:FindFirstChild("Humanoid") then
			local worldPosition = EnamyModel.HumanoidRootPart.Position
			local Humanoid = EnamyModel:FindFirstChild("Humanoid")
			local screenPosition, onScreen = Camera:WorldToViewportPoint(worldPosition)
			if Humanoid.Health > 0 then
				if onScreen then
					local mousePos = UIS:GetMouseLocation()
					local screenDistance = (Vector2.new(screenPosition.X, screenPosition.Y) - mousePos).Magnitude
					if screenDistance < minScreenDistance then
						local distance = (playerPosition - worldPosition).Magnitude
						if distance <= AimDis then
							minScreenDistance = screenDistance
							nearestZombie = EnamyModel
						end
					end
					if screenDistance <= screenRadius then
						closestZombie = EnamyModel
					end
				end
			end
		end
	end
	local NightEnemies = game.Workspace:FindFirstChild("NightEnemies")
	if NightEnemies and HMNRT then
		for _, zombie in ipairs(NightEnemies:GetChildren()) do
			GetNearFungtion(zombie)
		end
	end
	local RuntimeEnemies = game.Workspace:FindFirstChild("RuntimeEnemies")
	if RuntimeEnemies and HMNRT then
