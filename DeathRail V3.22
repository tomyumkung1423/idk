local function SetButtonImage(ButtonPart, Position, Size, Zindex, Parent, ImColor , Image)
	ButtonPart.Image = Image
	ButtonPart.Parent = Parent
	ButtonPart.Position = Position
	ButtonPart.Size = Size
	ButtonPart.BackgroundTransparency = 1
	ButtonPart.ZIndex = Zindex
	ButtonPart.ImageColor3 = ImColor
end
local function SetButtonText(ButtonPart, Position, Size, Zindex, Parent, BkColor , Text)
	local UICornerButtonPart = Instance.new("UICorner")
	ButtonPart.Text = Text
	ButtonPart.Parent = Parent
	ButtonPart.Position = Position
	ButtonPart.Size = Size
	ButtonPart.TextScaled = true
	ButtonPart.RichText = true
	ButtonPart.TextColor3 = Color3.new(1, 1, 1)
	if BkColor == 1 then
		ButtonPart.BackgroundTransparency = 1
		ButtonPart.TextColor3 = Color3.new(0.509804, 0.388235, 1)
	else
		ButtonPart.BackgroundColor3 = BkColor
	end
	ButtonPart.ZIndex = Zindex
	UICornerButtonPart.Parent = ButtonPart
end
local function NewFrame(FrameType, Position, Size, Zindex, Parent, BGC, In1, TorF)-----Huc = Have Ui Conner  ,  BGC = BackgroundColor3  , TorF = Ture Or False
	if TorF == nil then
		TorF = true
	end
	local ReturItem = nil
	if FrameType == "Frame" then
		local Item = Instance.new(FrameType)
		local UICornerFrame1 = Instance.new("UICorner")
		Item.Parent = Parent
		Item.Position = Position
		Item.Size = Size
		Item.BackgroundColor3 = BGC
		Item.ZIndex = Zindex
		Item.Visible = TorF
		UICornerFrame1.Parent = Item
		ReturItem = Item
	elseif FrameType == "TextLabel" then----In1 = text
		local Item = Instance.new("TextLabel")
		Item.Parent = Parent
		Item.Position = Position
		Item.Size = Size
		Item.BackgroundTransparency = 1
		Item.Font = Enum.Font.Highway
		Item.TextScaled = true
		Item.ZIndex = Zindex
		Item.RichText = true
		Item.TextXAlignment = Enum.TextXAlignment.Left
		Item.Text = In1
		Item.TextColor3 = BGC
		ReturItem = Item
	elseif FrameType == "ImageLabel" then----In1 = ImageId
		local Item = Instance.new(FrameType)
		Item.Image = In1
		Item.Parent = Parent
		Item.Position = Position
		Item.Size = Size
		Item.BackgroundTransparency = 1
		Item.ZIndex = Zindex
		Item.ImageColor3 = BGC
		ReturItem = Item
	end
	return ReturItem
end
