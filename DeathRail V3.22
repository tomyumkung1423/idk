local function updateSlider3(inputX, fill, knob, valueLabel, slider, minValue, maxValue)
	local sliderSize = slider.AbsoluteSize.X
	local sliderPos = slider.AbsolutePosition.X
	local relativeX = math.clamp(inputX - sliderPos, 0, sliderSize)
	local percent = relativeX / sliderSize
	Value27 = minValue + (maxValue - minValue) * percent
	fill.Size = UDim2.new(percent, 0, 1, 0)
	knob.Position = UDim2.new(percent, -knob.AbsoluteSize.X / 2, 0.5, -knob.AbsoluteSize.Y / 2)
	valueLabel.Text = "<b> ระยะมองทะลุ </b>" .. (math.round(Value27 * 0.28)) .. " เมตร "
	EspDistan = Value27
end
local function onInputBegan27(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		local moveConnection
		local releaseConnection
		moveConnection = UIS.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				updateSlider3(input.Position.X, Line27, LineButton27, TextLabelButtonCon27, MainLine27, minValue27, maxValue27)
				if input.UserInputType == Enum.UserInputType.Touch then
					Camera.CameraType = Enum.CameraType.Scriptable
				end
			end
		end)
		releaseConnection = UIS.InputEnded:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
				if moveConnection then moveConnection:Disconnect() moveConnection = nil end
				if releaseConnection then releaseConnection:Disconnect() releaseConnection = nil end
				if not AimbotHot then
					Camera.CameraType = Enum.CameraType.Custom
				end
			end
		end)
	end
end
