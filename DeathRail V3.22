local function HelpAttackfunction(nearestZombie, backpack)
	if LastAttack == 0 then
		if  nearestZombie and nearestZombie:FindFirstChild("Humanoid") and nearestZombie.Humanoid.Health > 0 then		
			local currentDistance = (HMNRT.Position - nearestZombie.HumanoidRootPart.Position).Magnitude
			if currentDistance < 13 then
				local shovel = backpack:FindFirstChild("Shovel")
				if shovel then
					shovel.Parent = character
				end
				if nearestZombie:FindFirstChild("Head") then
					Camera.CFrame = CFrame.new(Camera.CFrame.Position, nearestZombie.Head.Position)
				end
				local args = {[1] = Vector3.new(math.random(), math.random(), math.random())}
				game:GetService("Players").LocalPlayer.Character.Shovel.SwingEvent:FireServer(unpack(args))
				LastAttack += 0.01
			end
		end
	elseif LastAttack > 0 and 0.5 > LastAttack then
		LastAttack += 0.01
	else
		LastAttack = 0
	end
end
local function MobEsp(ModelMob, OpenBill)
	local Humanoid = ModelMob:FindFirstChild("Humanoid")
	local HumanoidRootPart = ModelMob:FindFirstChild("HumanoidRootPart")
	if Humanoid and HumanoidRootPart then
		local distance = (HMNRT.Position - HumanoidRootPart.Position).Magnitude
		local highlight = ModelMob:FindFirstChild("ZombieSHilight")
		local billboardGui = ModelMob:FindFirstChild("BillboardGui")
		local stringValue = ModelMob:FindFirstChild("RealName")
		if distance <= EspDistan then
			if not highlight then
				highlight = Instance.new("Highlight")
				highlight.FillColor = Color3.new(1, 0.164706, 0)
				highlight.OutlineTransparency = 1
				highlight.Name = "ZombieSHilight"
				highlight.Parent = ModelMob
				stringValue = Instance.new("StringValue")
				stringValue.Name = "RealName"
				stringValue.Parent = ModelMob
				billboardGui = Instance.new("BillboardGui")
				billboardGui.Size = UDim2.new(10, 0, 4, 0)
				billboardGui.StudsOffset = Vector3.new(0, 4.5, 0)
				billboardGui.AlwaysOnTop = true
				billboardGui.Parent = ModelMob
				local textLabel = Instance.new("TextLabel")
				textLabel.Size = UDim2.new(1, 0, 1, 0)
				textLabel.TextScaled = true
				textLabel.BackgroundTransparency = 1
				textLabel.TextStrokeTransparency = 0
				textLabel.Font = Enum.Font.Highway
				textLabel.RichText = true
				textLabel.Text = "<b> </b>"
				textLabel.Parent = billboardGui				
				if ModelMob.Name == "Walker" then
					highlight.FillColor = Color3.new(1, 0.701961, 0)
					stringValue.Value = "Walker"
				elseif ModelMob.Name == "Runner" then
					highlight.FillColor = Color3.new(1, 0.333333, 0)
					stringValue.Value = "Runner"
				else
					highlight.FillColor = Color3.new(0.447059, 0.227451, 1)
					stringValue.Value = ModelMob.Name
				end
			else
				local NameEsp = "ไม่สามารถหาชื่อได้"
				if stringValue then
					if stringValue.Value == "Runner" then
						NameEsp = "ซอมบี้วิ่งเร็ว"
					elseif stringValue.Value == "Walker" then
						NameEsp = "ซอมบี้ปกติ"
					else
						NameEsp = stringValue.Value
					end
				end
				local textLabel = billboardGui and billboardGui:FindFirstChild("TextLabel")
				if textLabel then
					local Meter = math.floor(distance * 0.28)
					textLabel.Text = Meter > 0 and (" " .. NameEsp .. " " .. Meter .. "<b> เมตร </b>") or NameEsp
					textLabel.TextColor3 = (NameEsp == "ซอมบี้วิ่งเร็ว ") and Color3.new(1, 0.368627, 0) or Color3.new(1, 0.717647, 0)
				end
				if OpenBill == false then
					billboardGui.Enabled = false
				else
					billboardGui.Enabled = true
				end
			end
		else
			if highlight then highlight:Destroy() end
			if billboardGui then billboardGui:Destroy() end
			if stringValue then stringValue:Destroy() end
		end
	end
end
