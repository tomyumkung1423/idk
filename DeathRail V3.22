local function EquipTool(ItemName)
	local backpack = player:FindFirstChild("Backpack")
	if character and character:FindFirstChildOfClass("Tool") then
		local currentTool = character:FindFirstChildOfClass("Tool")
		currentTool.Parent = backpack
	end
	local Item = backpack:FindFirstChild(ItemName)
	if Item then
		Item.Parent = character
	end
end
local function SetTarget(Button)
	RunTarget = true
	DisibleEsp()
	local NeedName = Button.Name
	if RunTargetFd:FindFirstChild("AllTarget") then
		RunTargetFd:FindFirstChild("AllTarget"):Destroy()
		ButtonRunTarget1.Text = "<b> มองทะลุทุกชิ้น ✅ </b>"
	end
	if not RunTargetFd:FindFirstChild(NeedName) then
		local TerGetValue = Instance.new("BoolValue")
		TerGetValue.Parent = RunTargetFd
		TerGetValue.Name = NeedName
		Button.Text = " " .. NeedName .. " ✅ "
	else
		RunTargetFd:FindFirstChild(NeedName):Destroy()
		Button.Text = NeedName
		--RunTarget = false
	end
end
