local function DisibleEsp()
	local runtimeItems = game.Workspace:FindFirstChild("RuntimeItems")
	for _, Item138 in ipairs(runtimeItems:GetChildren()) do
		if Item138:IsA("Model") then
			local EspHighlight = Item138:FindFirstChild("SEspHilight")
			local ZombieSHilight = Item138:FindFirstChild("ZombieSHilight")
			local EspPart = Item138:FindFirstChild("EspPart")
			local RealNameObj = Item138:FindFirstChild("RealName")
			if EspHighlight then EspHighlight:Destroy() end
			if ZombieSHilight then ZombieSHilight:Destroy() end
			local BillboardGui = Item138:FindFirstChild("BillboardGui")
			if BillboardGui then
				for _, Item943 in ipairs(Item138:GetChildren()) do
					if Item943.Name == "BillboardGui" then
						Item943:Destroy()
					end
				end
			end
			if RealNameObj then
				Item138.Name = RealNameObj.Value
			end
		end
	end
	local RandomBuildings = game.Workspace:FindFirstChild("RandomBuildings")
	for _, Item729 in ipairs(RandomBuildings:GetChildren()) do        
		if Item729:IsA("Model") then
			local StandaloneZombiePart = Item729:FindFirstChild("StandaloneZombiePart")
			if StandaloneZombiePart then                
				local Zombies = StandaloneZombiePart:FindFirstChild("Zombies")
				if Zombies then
					for _, Item816 in ipairs(Zombies:GetChildren()) do
						local ZombieSHilight = Item816:FindFirstChild("ZombieSHilight")
						local BillboardGui = Item816:FindFirstChild("BillboardGui")
						if ZombieSHilight then ZombieSHilight:Destroy() end
						if BillboardGui then BillboardGui:Destroy() end
					end
				end
			end
		end
	end
	local Towns = workspace:FindFirstChild("Towns")
	if Towns then
		for _, townType in ipairs({"SmallTownTemplate", "MediumTownTemplate", "LargeTownTemplate"}) do
			local town = Towns:FindFirstChild(townType)
			if town then
				local Zombies = town:FindFirstChild("ZombiePart"):FindFirstChild("Zombies")
				if Zombies then
					if Zombies then
						for _, Item816 in ipairs(Zombies:GetChildren()) do
							local ZombieSHilight = Item816:FindFirstChild("ZombieSHilight")
							local BillboardGui = Item816:FindFirstChild("BillboardGui")
							if ZombieSHilight then ZombieSHilight:Destroy() end
							if BillboardGui then BillboardGui:Destroy() end
						end
					end
				end
			end
		end
	end
	local NightEnemies = workspace:FindFirstChild("NightEnemies")
	if NightEnemies then
		for _, Item816 in ipairs(NightEnemies:GetChildren()) do
			local ZombieSHilight = Item816:FindFirstChild("ZombieSHilight")
			local BillboardGui = Item816:FindFirstChild("BillboardGui")
			if ZombieSHilight then ZombieSHilight:Destroy() end
			if BillboardGui then BillboardGui:Destroy() end
		end
	end
	local RuntimeEnemies = workspace:FindFirstChild("RuntimeEnemies")
	if RuntimeEnemies then
		for _, Item816 in ipairs(RuntimeEnemies:GetChildren()) do
			local ZombieSHilight = Item816:FindFirstChild("ZombieSHilight")
			local BillboardGui = Item816:FindFirstChild("BillboardGui")
			if ZombieSHilight then ZombieSHilight:Destroy() end
			if BillboardGui then BillboardGui:Destroy() end
		end
	end
	local Baseplates = workspace:FindFirstChild("Baseplates")
	if Baseplates then
		for _, Model in ipairs(Baseplates:GetChildren()) do
			if Model:IsA("Model") and Model:FindFirstChild("CenterBaseplate") then
				local CenterBaseplate = Model:FindFirstChild("CenterBaseplate")
				if CenterBaseplate:FindFirstChild("Animals") then
					local Animals = CenterBaseplate:FindFirstChild("Animals")
					for _, Item816 in ipairs(Animals:GetChildren()) do
						local ZombieSHilight = Item816:FindFirstChild("ZombieSHilight")
						local BillboardGui = Item816:FindFirstChild("BillboardGui")
						if ZombieSHilight then ZombieSHilight:Destroy() end
						if BillboardGui then BillboardGui:Destroy() end
					end
				end
			end
		end
	end
end
